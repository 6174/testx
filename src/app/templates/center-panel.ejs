<div id="TxCenterPanel" class="center-panel panel ">
  <!-- control -->
  <div class="panel-heading">
    <div id="TxProjectControl" class="control">
      <!-- test  platform select -->
      <div class="btn-group">
        <button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown">
        platform<span class="caret"></span>
        </button>
        <ul class="dropdown-menu" role="menu">
          <li><a href="#">chrome</a></li>
          <li><a href="#">firefox</a></li>
          <li><a href="#">phantomJs</a></li>
        </ul>
      </div>
      <!-- test run button -->
      <button id="TestRun" class="btn pull-right" type="button">Run</button>
    </div>
  </div>

  <!-- playground -->
  <div class="panel-body">
    <!-- test progress bar -->
    <div id="TxProgressbar"  class="progress">
      <div class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: 60%;">
        <span class="sr-only">60% Complete</span>
      </div>
    </div>
    <div id="TxPlayground" class="playground custom-scrollbar">
      <% include test-suit/test-suit %>
      <% include test-suit/test-suit %>
       <% include test-suit/test-suit %>
        <% include test-suit/test-suit %>
    </div>
  </div>
</div>